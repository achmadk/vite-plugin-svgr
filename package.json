{
  "name": "@achmadk/vite-plugin-svgr",
  "private": false,
  "version": "2.0.0",
  "description": "Vite plugin to transform SVGs into React components",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "scripts": {
    "dev": "tsdv watch",
    "build": "tsdv build",
    "prepare": "npm run build",
    "prepublishOnly": "pnpm run build"
  },
  "repository": "achmadk/vite-plugin-svgr",
  "files": [
    "dist",
    "client.d.ts"
  ],
  "keywords": [
    "vite",
    "vite-plugin"
  ],
  "author": "Achmad Kurnianto",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.12",
    "react": "^18.2.0",
    "tsdv": "^0.8.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  },
  "peerDependencies": {
    "vite": "^2.6.0 || 3 || 4"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "@svgr/core": "^8.0.0",
    "@svgr/plugin-jsx": "^8.0.1"
  },
  "packageManager": "pnpm@8.6.2"
}
